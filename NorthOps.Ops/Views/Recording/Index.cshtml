
@{
    ViewBag.Title = "Coaching Logs";
    Layout = "~/Views/Shared/_layout.cshtml";
}



@Html.Action("RecordingGridViewPartial")




<script>
    function onFileUploadComplete(e) {
        if (e.callbackData) {
            var fileData = e.callbackData.split('|');
            var fileName = fileData[0];
            $('#lbl-fileName').text(fileName);
            console.log(fileName);
        }
        console.log(e);
    }
</script>

<script>
    function checkPassword(pwd) {
        btnConfirm.SetEnabled(false);
        $.ajax({
            url: "@Url.Action("CheckPassword")",
            data: { password: pwd },
            success: function(e) {
                if (e == true) {
                    btnConfirm.SetEnabled(true);
                }
            }

        });

    }
    function checkValue() {
        if (CallerNumber.GetValue() == null ||
            CallDate.GetValue() == null ||
            ErrorTypeId.GetValue() == null ||
            Errors.GetValue() == null ||
            RootCauseAnalysis.GetValue() == null ||
            Commitment.GetValue() == null) {

            btnUpdate.SetEnabled(false);


        } else {
            btnUpdate.SetEnabled(true);
        }



    }


</script>